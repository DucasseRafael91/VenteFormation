@startuml VenteFormation

class Formation {
    -int id
    -string nom
    -text description
    -int duree_jours
    -double prix
}

class TypeFormation {
    -int id
    -string nom
}

class Categorie {
    -int id
    -string nom
}

class Utilisateur {
    -int id
    -string identifiant
    -string mot_de_passe
}

class Client {
    -int id
    -string nom
    -string prenom
    -string adresse
    -string telephone
}

class Commande {
    -int id
    -date date_commande
}

